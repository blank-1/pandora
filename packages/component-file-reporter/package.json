{
  "name": "@pandorajs/component-file-reporter",
  "version": "2.2.0",
  "description": "Pandora 2 Component File Reporter",
  "main": "build/src/ComponentFileReporter.js",
  "typings": "build/src/ComponentFileReporter.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "test": "#SKIP NODE_ENV=unittest mocha --trace-warnings",
    "cov": "nyc mocha",
    "ci": "npm run test"
  },
  "keywords": [
    "pandora"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.0.0",
    "chai": "^4.2.0",
    "mocha": "^7.1.2",
    "nyc": "^13.1.0"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "typings",
      "dist"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.d.ts"
  ],
  "dependencies": {
    "@opentelemetry/api": "^0.8.3",
    "@opentelemetry/tracing": "^0.8.3",
    "@pandorajs/component-decorator": "^2.1.2",
    "@pandorajs/component-file-logger-service": "^2.2.0",
    "@pandorajs/component-logger": "^2.2.0",
    "@pandorajs/component-metric": "^2.2.0",
    "@pandorajs/dollar": "^2.1.2",
    "md5": "^2.2.1"
  }
}
